<?xml version="1.0"?>
<launch>

	<!-- hztest scan_front_raw -->
	<param name="hztest_scan_front_raw/topic" value="scan_front_raw" />
	<param name="hztest_scan_front_raw/hz" value="5.0" />
	<param name="hztest_scan_front_raw/hzerror" value="1.0" />
	<param name="hztest_scan_front_raw/test_duration" value="5.0" />
