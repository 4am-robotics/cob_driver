<?xml version="1.0"?>
<launch>

  	<!-- start base_drive_chain and locate IniFiles -->
    <node pkg="cob_base_drive_chain" type="cob_base_drive_chain_node" name="base_drive_chain" respawn="false" output="screen">
		<param name="IniDirectory" value="$(find cob_platform)/ros/bin/Platform/IniFiles/"/>
		<param name="PublishEffort" value="true"/>
	</node>
    

</launch>
